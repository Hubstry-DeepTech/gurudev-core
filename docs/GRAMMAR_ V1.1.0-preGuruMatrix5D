  overscript_attribute WHITESPACE
} SOBRESCRITA_END ;

overscript_attribute = (
  STRING_LITERAL
  | NIVEL_ATTR
  | RAIZ_ATTR
  | CLAVE_ATTR
  | ONT_ATTR
